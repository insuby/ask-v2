{
  "compilerOptions": {
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@constants": [
        "src/constants/constants.tsx"
      ],
      "@hooks": [
        "src/hooks/hooks.ts"
      ],
      "@stylePages/*": [
        "src/pages/create/*"
      ],
      "@components": [
        "src/components/index.ts"
      ],
      "@componentsPages": [
        "src/pages/create/components/index.ts"
      ],
      "@i18n": [
        "src/i18n/i18n.ts"
      ],
      "@icons": [
        "src/icons/icons.ts"
      ],
      "@store": [
        "src/store/store.ts"
      ],
      "@types": [
        "src/types/types.ts"
      ],
      "@config": [
        "./config.ts"
      ],
      "@common": [
        "src/common/index.ts"
      ],
      "@tron": [
        "src/tron/index.ts"
      ],
      "@pages/*": [
        "src/pages/*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}